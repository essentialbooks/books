        "properties": {
          "title": [
            [
              "When doing CPU intensive tasks like image resizing it doesn’t make sense to create more goroutines than available CPUs."
            ]
          ]
        },
        "type": "text",
        "version": 12,
        "inline_content": [
          {
            "Text": "When doing CPU intensive tasks like image resizing it doesn’t make sense to create more goroutines than available CPUs."
          }
        ]
      },
