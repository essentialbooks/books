        "properties": {
          "title": [
            [
              "Checking if a file exists is surprisingly tricky and it’s impossible to write a generic function that handles all the nuances."
            ]
          ]
        },
        "type": "text",
        "version": 1,
        "inline_content": [
          {
            "Text": "Checking if a file exists is surprisingly tricky and it’s impossible to write a generic function that handles all the nuances."
          }
        ]
      },
      {
        "alive": true,
        "created_by": "bb760e2d-d679-4b64-b2a9-03005b21870a",
